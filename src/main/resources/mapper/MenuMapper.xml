  <!--查询对应用户的所有权限 -->
	<select id="selectPermission" resultMap="BaseResultMap">
		SELECT M.* FROM employees e INNER JOIN role r ON e.rID=r.ID INNER JOIN role_permission rp ON r.id=rp.rid INNER JOIN menu m ON rp.mid=m.id WHERE e.ID=#{id}
	</select>
	<!--用户登录所见菜单-->
	<select id="selectMenuByUid" resultMap="BaseResultMap">
		SELECT m.* FROM `user` u INNER JOIN role r ON u.rID=r.ID INNER JOIN role_permission rp ON r.id=rp.rid INNER JOIN menu m ON rp.mid=m.id WHERE u.ID=#{id}
	</select>
